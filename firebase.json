{
  "hosting": {
    "public": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "hotel-proxy-server.js",
      "serpapi-proxy-server.js",
      "start-services.sh",
      "package.json",
      "package-lock.json",
      "**/*.md"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/admin-dashboard.html"
      }
    ],
    "headers": [
      {
        "source": "**/*.@(js|css)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=31536000"
          }
        ]
      }
    ]
  }
}